						if (document.getElementById('statusLahir').value=="Hidup") {

							$("#formMati").hide();
							$("#formLahir").show();
						}
						else if (document.getElementById('statusLahir').value=="Mati") {
							
							$("#formMati").show();
							$("#formLahir").hide();
						}else{
									
							$("#formMati").hide();
							$("#formLahir").hide();
						}
					});
